<!-- Dashboard Statistics Cards -->
<div class="p-4 sm:p-6 lg:p-8">
  <div class="max-w-7xl mx-auto">
    <!-- Header -->
    <div class="mb-8">
      <h1 class="text-3xl font-bold text-gray-900 dark:text-white">
        Dashboard Overview
      </h1>
      <p class="mt-2 text-gray-600 dark:text-gray-400">
        Welcome back! Here's what's happening with your application.
      </p>
    </div>

    <!-- Statistics Grid -->
    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6 mb-8">
      <!-- Total Users Card -->
      <div class="bg-gradient-to-br from-blue-500 to-blue-600 rounded-xl p-6 text-white shadow-lg">
        <div class="flex items-center justify-between">
          <div>
            <p class="text-blue-100 text-sm font-medium">Total Users</p>
            <p class="text-3xl font-bold"><%= stats.users.total %></p>
            <p class="text-blue-100 text-sm mt-1">
              <span class="inline-flex items-center">
                <svg class="w-4 h-4 mr-1" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M5.293 7.707a1 1 0 010-1.414l4-4a1 1 0 011.414 0l4 4a1 1 0 01-1.414 1.414L11 5.414V17a1 1 0 11-2 0V5.414L6.707 7.707a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
                </svg>
                +<%= stats.users.growth %>%
              </span>
            </p>
          </div>
          <div class="bg-blue-400 bg-opacity-30 rounded-full p-3">
            <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
              <path d="M9 6a3 3 0 11-6 0 3 3 0 016 0zM17 6a3 3 0 11-6 0 3 3 0 016 0zM12.93 17c.046-.327.07-.66.07-1a6.97 6.97 0 00-1.5-4.33A5 5 0 0119 16v1h-6.07zM6 11a5 5 0 015 5v1H1v-1a5 5 0 015-5z"></path>
            </svg>
          </div>
        </div>
      </div>

      <!-- Shops Card -->
      <div class="bg-gradient-to-br from-green-500 to-green-600 rounded-xl p-6 text-white shadow-lg">
        <div class="flex items-center justify-between">
          <div>
            <p class="text-green-100 text-sm font-medium">Shops</p>
            <p class="text-3xl font-bold"><%= stats.users.shops %></p>
            <p class="text-green-100 text-sm mt-1">
              <span class="inline-flex items-center">
                <svg class="w-4 h-4 mr-1" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M5.293 7.707a1 1 0 010-1.414l4-4a1 1 0 011.414 0l4 4a1 1 0 01-1.414 1.414L11 5.414V17a1 1 0 11-2 0V5.414L6.707 7.707a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
                </svg>
                +<%= stats.growth.shops %>%
              </span>
            </p>
          </div>
          <div class="bg-green-400 bg-opacity-30 rounded-full p-3">
            <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M4 4a2 2 0 00-2 2v8a2 2 0 002 2h12a2 2 0 002-2V6a2 2 0 00-2-2H4zm2 6a2 2 0 114 0 2 2 0 01-4 0zm8 0a2 2 0 114 0 2 2 0 01-4 0z" clip-rule="evenodd"></path>
            </svg>
          </div>
        </div>
      </div>

      <!-- Professionals Card -->
      <div class="bg-gradient-to-br from-purple-500 to-purple-600 rounded-xl p-6 text-white shadow-lg">
        <div class="flex items-center justify-between">
          <div>
            <p class="text-purple-100 text-sm font-medium">Professionals</p>
            <p class="text-3xl font-bold"><%= stats.users.professionals %></p>
            <p class="text-purple-100 text-sm mt-1">
              <span class="inline-flex items-center">
                <svg class="w-4 h-4 mr-1" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M5.293 7.707a1 1 0 010-1.414l4-4a1 1 0 011.414 0l4 4a1 1 0 01-1.414 1.414L11 5.414V17a1 1 0 11-2 0V5.414L6.707 7.707a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
                </svg>
                +<%= stats.growth.professionals %>%
              </span>
            </p>
          </div>
          <div class="bg-purple-400 bg-opacity-30 rounded-full p-3">
            <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z" clip-rule="evenodd"></path>
            </svg>
          </div>
        </div>
      </div>

      <!-- Active Users Card -->
      <div class="bg-gradient-to-br from-orange-500 to-orange-600 rounded-xl p-6 text-white shadow-lg">
        <div class="flex items-center justify-between">
          <div>
            <p class="text-orange-100 text-sm font-medium">Active Users</p>
            <p class="text-3xl font-bold"><%= stats.users.active %></p>
            <p class="text-orange-100 text-sm mt-1">
              <span class="inline-flex items-center">
                <svg class="w-4 h-4 mr-1" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                Active
              </span>
            </p>
          </div>
          <div class="bg-orange-400 bg-opacity-30 rounded-full p-3">
            <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-8.293l-3-3a1 1 0 00-1.414 0l-3 3a1 1 0 001.414 1.414L9 9.414V13a1 1 0 102 0V9.414l1.293 1.293a1 1 0 001.414-1.414z" clip-rule="evenodd"></path>
            </svg>
          </div>
        </div>
      </div>
    </div>

    <!-- Master Tables Statistics -->
    <div class="grid grid-cols-1 lg:grid-cols-4 gap-6 mb-8">
      <!-- Shop Types Card -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-lg font-semibold text-gray-900 dark:text-white">Shop Types</h3>
          <div class="bg-blue-100 dark:bg-blue-900 rounded-full p-2">
            <svg class="w-6 h-6 text-blue-600 dark:text-blue-400" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M4 4a2 2 0 00-2 2v8a2 2 0 002 2h12a2 2 0 002-2V6a2 2 0 00-2-2H4zm2 6a2 2 0 114 0 2 2 0 01-4 0zm8 0a2 2 0 114 0 2 2 0 01-4 0z" clip-rule="evenodd"></path>
            </svg>
          </div>
        </div>
        <div class="space-y-2">
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Total</span>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.masterTables.shopTypes.total %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Active</span>
            <span class="font-semibold text-green-600 dark:text-green-400"><%= stats.masterTables.shopTypes.active %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Inactive</span>
            <span class="font-semibold text-red-600 dark:text-red-400"><%= stats.masterTables.shopTypes.total - stats.masterTables.shopTypes.active %></span>
          </div>
        </div>
      </div>

      <!-- Amenities Card -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-lg font-semibold text-gray-900 dark:text-white">Amenities</h3>
          <div class="bg-yellow-100 dark:bg-yellow-900 rounded-full p-2">
            <svg class="w-6 h-6 text-yellow-600 dark:text-yellow-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 3v4M3 5h4M6 17v4m-2-2h4m5-16l2.286 6.857L21 12l-5.714 2.143L13 21l-2.286-6.857L5 12l5.714-2.143L13 3z"></path>
            </svg>
          </div>
        </div>
        <div class="space-y-2">
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Total</span>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.masterTables.amenities.total %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Active</span>
            <span class="font-semibold text-green-600 dark:text-green-400"><%= stats.masterTables.amenities.active %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Inactive</span>
            <span class="font-semibold text-red-600 dark:text-red-400"><%= stats.masterTables.amenities.total - stats.masterTables.amenities.active %></span>
          </div>
        </div>
      </div>

      <!-- Services Card -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-lg font-semibold text-gray-900 dark:text-white">Services</h3>
          <div class="bg-green-100 dark:bg-green-900 rounded-full p-2">
            <svg class="w-6 h-6 text-green-600 dark:text-green-400" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M3 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zm0 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zm0 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zm0 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path>
            </svg>
          </div>
        </div>
        <div class="space-y-2">
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Total</span>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.masterTables.services.total %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Active</span>
            <span class="font-semibold text-green-600 dark:text-green-400"><%= stats.masterTables.services.active %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Inactive</span>
            <span class="font-semibold text-red-600 dark:text-red-400"><%= stats.masterTables.services.total - stats.masterTables.services.active %></span>
          </div>
        </div>
      </div>

      <!-- Professional Skills Card -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-lg font-semibold text-gray-900 dark:text-white">Professional Skills</h3>
          <div class="bg-purple-100 dark:bg-purple-900 rounded-full p-2">
            <svg class="w-6 h-6 text-purple-600 dark:text-purple-400" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M6.267 3.455a3.066 3.066 0 001.745-.723 3.066 3.066 0 013.976 0 3.066 3.066 0 001.745.723 3.066 3.066 0 012.812 2.812c.051.643.304 1.254.723 1.745a3.066 3.066 0 010 3.976 3.066 3.066 0 00-.723 1.745 3.066 3.066 0 01-2.812 2.812 3.066 3.066 0 00-1.745.723 3.066 3.066 0 01-3.976 0 3.066 3.066 0 00-1.745-.723 3.066 3.066 0 01-2.812-2.812 3.066 3.066 0 00-.723-1.745 3.066 3.066 0 010-3.976 3.066 3.066 0 00.723-1.745 3.066 3.066 0 012.812-2.812zm7.44 5.252a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
            </svg>
          </div>
        </div>
        <div class="space-y-2">
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Total</span>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.masterTables.professionalSkills.total %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Active</span>
            <span class="font-semibold text-green-600 dark:text-green-400"><%= stats.masterTables.professionalSkills.active %></span>
          </div>
          <div class="flex justify-between">
            <span class="text-gray-600 dark:text-gray-400">Inactive</span>
            <span class="font-semibold text-red-600 dark:text-red-400"><%= stats.masterTables.professionalSkills.total - stats.masterTables.professionalSkills.active %></span>
          </div>
        </div>
      </div>
    </div>

    <!-- Content & Access Control Statistics -->
    <div class="grid grid-cols-1 lg:grid-cols-2 gap-6 mb-8">
      <!-- Content Management -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <h3 class="text-lg font-semibold text-gray-900 dark:text-white mb-4">Content Management</h3>
        <div class="space-y-3">
          <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
            <div class="flex items-center">
              <div class="bg-blue-100 dark:bg-blue-900 rounded-full p-2 mr-3">
                <svg class="w-5 h-5 text-blue-600 dark:text-blue-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M4 4a2 2 0 012-2h4.586A2 2 0 0112 2.586L15.414 6A2 2 0 0116 7.414V16a2 2 0 01-2 2H6a2 2 0 01-2-2V4zm2 6a1 1 0 011-1h6a1 1 0 110 2H7a1 1 0 01-1-1zm1 3a1 1 0 100 2h6a1 1 0 100-2H7z" clip-rule="evenodd"></path>
                </svg>
              </div>
              <span class="text-gray-700 dark:text-gray-300">Pages</span>
            </div>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.content.pages %></span>
          </div>
          <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
            <div class="flex items-center">
              <div class="bg-green-100 dark:bg-green-900 rounded-full p-2 mr-3">
                <svg class="w-5 h-5 text-green-600 dark:text-green-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-8-3a1 1 0 00-.867.5 1 1 0 11-1.731-1A3 3 0 0113 8a3.001 3.001 0 01-2 2.83V11a1 1 0 11-2 0v-1a1 1 0 011-1 1 1 0 100-2zm0 8a1 1 0 100-2 1 1 0 000 2z" clip-rule="evenodd"></path>
                </svg>
              </div>
              <span class="text-gray-700 dark:text-gray-300">FAQs</span>
            </div>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.content.faqs %></span>
          </div>
          <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
            <div class="flex items-center">
              <div class="bg-purple-100 dark:bg-purple-900 rounded-full p-2 mr-3">
                <svg class="w-5 h-5 text-purple-600 dark:text-purple-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M3 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zm0 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zm0 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zm0 4a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path>
                </svg>
              </div>
              <span class="text-gray-700 dark:text-gray-300">Categories</span>
            </div>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.content.categories %></span>
          </div>
        </div>
      </div>

      <!-- Access Control -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <h3 class="text-lg font-semibold text-gray-900 dark:text-white mb-4">Access Control</h3>
        <div class="space-y-3">
          <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
            <div class="flex items-center">
              <div class="bg-orange-100 dark:bg-orange-900 rounded-full p-2 mr-3">
                <svg class="w-5 h-5 text-orange-600 dark:text-orange-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z" clip-rule="evenodd"></path>
                </svg>
              </div>
              <span class="text-gray-700 dark:text-gray-300">Roles</span>
            </div>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.accessControl.roles %></span>
          </div>
          <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
            <div class="flex items-center">
              <div class="bg-red-100 dark:bg-red-900 rounded-full p-2 mr-3">
                <svg class="w-5 h-5 text-red-600 dark:text-red-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M5 9V7a5 5 0 0110 0v2a2 2 0 012 2v5a2 2 0 01-2 2H5a2 2 0 01-2-2v-5a2 2 0 012-2zm8-2v2H7V7a3 3 0 016 0z" clip-rule="evenodd"></path>
                </svg>
              </div>
              <span class="text-gray-700 dark:text-gray-300">Permissions</span>
            </div>
            <span class="font-semibold text-gray-900 dark:text-white"><%= stats.accessControl.permissions %></span>
          </div>
        </div>
      </div>
    </div>

    <!-- Recent Activity -->
    <div class="grid grid-cols-1 lg:grid-cols-3 gap-6">
      <!-- Recent Users -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <h3 class="text-lg font-semibold text-gray-900 dark:text-white mb-4">Recent Users</h3>
        <div class="space-y-3">
          <% if (stats.recent.users.length > 0) { %>
            <% stats.recent.users.forEach(function(user) { %>
              <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
                <div class="flex items-center">
                  <div class="w-8 h-8 bg-gradient-to-br from-blue-400 to-blue-600 rounded-full flex items-center justify-center text-white text-sm font-semibold mr-3">
                    <%= (user.name && user.name.length > 0) ? user.name.charAt(0).toUpperCase() : (user.email ? user.email.charAt(0).toUpperCase() : 'U') %>
                  </div>
                  <div>
                    <p class="text-sm font-medium text-gray-900 dark:text-white"><%= user.name || 'User' %></p>
                    <p class="text-xs text-gray-500 dark:text-gray-400"><%= user.email %></p>
                  </div>
                </div>
                <span class="text-xs text-gray-500 dark:text-gray-400">
                  <%= new Date(user.createdAt).toLocaleDateString() %>
                </span>
              </div>
            <% }); %>
          <% } else { %>
            <p class="text-gray-500 dark:text-gray-400 text-center py-4">No recent users</p>
          <% } %>
        </div>
      </div>

      <!-- Recent Shop Types -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <h3 class="text-lg font-semibold text-gray-900 dark:text-white mb-4">Recent Shop Types</h3>
        <div class="space-y-3">
          <% if (stats.recent.shopTypes.length > 0) { %>
            <% stats.recent.shopTypes.forEach(function(shopType) { %>
              <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
                <div class="flex items-center">
                  <div class="w-8 h-8 bg-gradient-to-br from-green-400 to-green-600 rounded-full flex items-center justify-center text-white text-sm font-semibold mr-3">
                    <%= shopType.name.charAt(0).toUpperCase() %>
                  </div>
                  <div>
                    <p class="text-sm font-medium text-gray-900 dark:text-white"><%= shopType.name %></p>
                    <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium <%= shopType.status ? 'bg-green-100 text-green-800 dark:bg-green-900 dark:text-green-200' : 'bg-red-100 text-red-800 dark:bg-red-900 dark:text-red-200' %>">
                      <%= shopType.status ? 'Active' : 'Inactive' %>
                    </span>
                  </div>
                </div>
                <span class="text-xs text-gray-500 dark:text-gray-400">
                  <%= new Date(shopType.createdAt).toLocaleDateString() %>
                </span>
              </div>
            <% }); %>
          <% } else { %>
            <p class="text-gray-500 dark:text-gray-400 text-center py-4">No recent shop types</p>
          <% } %>
        </div>
      </div>

      <!-- Recent Amenities -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <h3 class="text-lg font-semibold text-gray-900 dark:text-white mb-4">Recent Amenities</h3>
        <div class="space-y-3">
          <% if (stats.recent.amenities && stats.recent.amenities.length > 0) { %>
            <% stats.recent.amenities.forEach(function(amenity) { %>
              <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
                <div class="flex items-center">
                  <div class="w-8 h-8 bg-gradient-to-br from-yellow-400 to-yellow-600 rounded-full flex items-center justify-center text-white text-sm font-semibold mr-3">
                    <%= amenity.name.charAt(0).toUpperCase() %>
                  </div>
                  <div>
                    <p class="text-sm font-medium text-gray-900 dark:text-white"><%= amenity.name %></p>
                    <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium <%= amenity.status ? 'bg-green-100 text-green-800 dark:bg-green-900 dark:text-green-200' : 'bg-red-100 text-red-800 dark:bg-red-900 dark:text-red-200' %>">
                      <%= amenity.status ? 'Active' : 'Inactive' %>
                    </span>
                  </div>
                </div>
                <span class="text-xs text-gray-500 dark:text-gray-400">
                  <%= new Date(amenity.createdAt).toLocaleDateString() %>
                </span>
              </div>
            <% }); %>
          <% } else { %>
            <p class="text-gray-500 dark:text-gray-400 text-center py-4">No recent amenities</p>
          <% } %>
        </div>
      </div>

      <!-- Recent Services -->
      <div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg p-6">
        <h3 class="text-lg font-semibold text-gray-900 dark:text-white mb-4">Recent Services</h3>
        <div class="space-y-3">
          <% if (stats.recent.services.length > 0) { %>
            <% stats.recent.services.forEach(function(service) { %>
              <div class="flex items-center justify-between p-3 bg-gray-50 dark:bg-gray-700 rounded-lg">
                <div class="flex items-center">
                  <div class="w-8 h-8 bg-gradient-to-br from-purple-400 to-purple-600 rounded-full flex items-center justify-center text-white text-sm font-semibold mr-3">
                    <%= service.name.charAt(0).toUpperCase() %>
                  </div>
                  <div>
                    <p class="text-sm font-medium text-gray-900 dark:text-white"><%= service.name %></p>
                    <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium <%= service.status ? 'bg-green-100 text-green-800 dark:bg-green-900 dark:text-green-200' : 'bg-red-100 text-red-800 dark:bg-red-900 dark:text-red-200' %>">
                      <%= service.status ? 'Active' : 'Inactive' %>
                    </span>
                  </div>
                </div>
                <span class="text-xs text-gray-500 dark:text-gray-400">
                  <%= new Date(service.createdAt).toLocaleDateString() %>
                </span>
              </div>
            <% }); %>
          <% } else { %>
            <p class="text-gray-500 dark:text-gray-400 text-center py-4">No recent services</p>
          <% } %>
        </div>
      </div>
    </div>
  </div>
</div>
